{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "src": "./",
  "lang": "resources/lang",
  "locales": ["en", "ar"],
  "extensions": ["php", "blade.php", "ts", "tsx", "js", "jsx", "vue"],

  "showOrphans": true,
  "failOnOrphans": false,
  "showDuplicates": true,

  "checkParams": true,
  "checkPlurals": true,
  "checkFileDuplicates": true,
  "respectGitignore": false,

  "cache": false,
  "verbose": false,
  "json": false,

  "ignoreKeys": [
    "passwords.reset",
    "passwords.throttled",
    "passwords.token",
    "passwords.user"
  ],

  "ignorePatterns": [],

  "ignoreDomains": []
}
